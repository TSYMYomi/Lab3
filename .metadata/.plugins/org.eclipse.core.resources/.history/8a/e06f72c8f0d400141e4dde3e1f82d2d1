import java.util.ArrayList;
import java.util.List;


public class PrimeNumbers {

	public static List<Integer> primes(int i) {
		ArrayList<Integer> primeNums = new ArrayList<Integer>();
		primeNums.add(2);
		int x = 1;
		while(x < i) {
			if(i % 2 == 1) {
				primeNums.add(i);
			}
			i++;
		}
		return primeNums;
	}

}
